var server = http.createServer(function (req, res) {
    if (req.url === '/beep') {
        res.setHeader('content-type', 'text/plain');
        res.setHeader('foo', 'bar');
        res.setHeader('bling', req.headers.bling + '-blong');
        
        res.end('boop');
    }
    else ecstatic(req, res);
});

console.log('Listening on :8082');
server.listen(8082);


