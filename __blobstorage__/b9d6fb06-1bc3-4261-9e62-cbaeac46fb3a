for (var ix = 0; ix < pending; ix++) (function (count) {
  var expected =  {}
    , stringify = JSONStream.stringifyObject()
    
  es.connect(
    stringify,
    es.writeArray(function (err, lines) {
      it(JSON.parse(lines.join(''))).deepEqual(expected)
