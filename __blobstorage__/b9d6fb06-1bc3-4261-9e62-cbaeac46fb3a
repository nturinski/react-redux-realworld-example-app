if (this['window'] !== this) {
    assert = require("assert");
    Base64 = require('../base64.js').Base64;
}
var is = function (a, e, m) {
    return function () {
        assert.equal(a, e, m)
    }
};
var seed = function () {
    var a, i;
    for (a = [], i = 0; i < 256; i++) {
        a.push(String.fromCharCode(i));
    }
    return a.join('');
}();
describe('Base64', function () {
