_imp_VirtualQuery␀__imp__initterm␀mingw_initltsdyn_force␀__imp_fclose␀_dowildcard␀__imp___iob_func␀jpeg_finish_compress␀__dyn_tls_init_callback␀_initterm␀__imp_strncmp␀__major_image_version__␀_head_libjpeg_62_dll␀__loader_flags__␀___chkstk_ms␀__native_startup_lock␀__imp_jpeg_write_coefficients␀__mingw_winmain_hInstance␀GetStartupInfoA␀GetCurrentThreadId␀__rt_psrelocs_end␀__imp_jpeg_finish_compress␀__imp__get_invalid_parameter_handler␀__minor_subsystem_version__␀__minor_image_version__␀__imp__unlock␀__imp___set_app_type␀mingw_initltsdrot_force␀jpeg_stdio_dest␀jpeg_save_markers␀__imp_jpeg_read_header␀__imp_jpeg_add_quant_table␀__imp_isspace␀jcopy_block_row␀jpeg_write_coefficients␀DeleteCriticalSection␀__imp_RtlCaptureContext␀__imp_jpeg_std_error␀__RUNTIME_PSEUDO_RELOC_LIST_END__␀__imp_fopen␀__dllonexit␀__imp_GetCurrentProcess␀___crt_xt_end__␀__lib64_libmsvcrt_a_iname␀__imp_getc␀__fu0__set_invalid_parameter_handler␀__security_cookie␀/*jshint -W054 */
;(function (exports) {
  'use strict';

  function forEachAsync(arr, fn, thisArg) {
    var dones = []
      , index = -1
      ;

    function next(BREAK, result) {
      index += 1;

      if (index === arr.length || BREAK === forEachAsync.__BREAK) {
        dones.forEach(function (done) {
          done.call(thisArg, result);
        });
        return;
      }

      fn.call(thisArg, next, arr[index], index, arr);
    }

    setTimeout(next, 4);

    return {
      then: function (_done) {
        dones.push(_done);
        return this;
      }
    };
  }
  forEachAsync.__BREAK = {};

  exports.forEachAsync = forEachAsync;
}('undefined' !== typeof exports && exports || new Function('return this')()));
var through = require('through2');

module.exports = function () {
  return through.obj();
};
