  "_inBundle": false,
  "_integrity": "sha512-UjgapumWlbMhkBgzT7Ykc5YXUT46F0iKu8SGXq0bcwP5dz/h0Plj6enJqjz1Zbq2l5WaqYnrVbwWOWMyF3F47g==",
  "_location": "/concat-with-sourcemaps/source-map",
  "_phantomChildren": {},
  "_requested": {
    "type": "range",
