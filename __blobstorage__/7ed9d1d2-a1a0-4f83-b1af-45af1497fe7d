if (Test-Path "$basedir/node$exe") {
  & "$basedir/node$exe"  "$basedir/../uglify-js/bin/uglifyjs" $args
  $ret=$LASTEXITCODE
} else {
  & "node$exe"  "$basedir/../uglify-js/bin/uglifyjs" $args
  $ret=$LASTEXITCODE
}
exit $ret
