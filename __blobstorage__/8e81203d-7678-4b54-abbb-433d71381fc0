var $export = require('./$.export');

$export($export.S, 'Number', {isInteger: require('./$.is-integer')});# os-filter-obj [![Build Status](http://img.shields.io/travis/kevva/os-filter-obj.svg?style=flat)](https://travis-ci.org/kevva/os-filter-obj)

> Filter an array of objects to a specific OS

## Install

```sh
$ npm install --save os-filter-obj
```

## Usage

```js
var osFilterObj = require('os-filter-obj');

var arr = [{
	foo: 'linux-x64',
	os: 'linux',
	arch: 'x64'
}, {
	foo: 'darwin-x64',
	os: 'darwin',
	arch: 'x64'
},{
	foo: 'win32-x64',
	os: 'win32',
	arch: 'x64'
}];

osFilterObj(arr);
//=> [{ foo: 'linux-x64' }];
```

