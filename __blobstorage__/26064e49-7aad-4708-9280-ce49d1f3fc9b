  "_from": "gulp-useref@^1.1.1",
  "_id": "gulp-useref@1.3.0",
  "_inBundle": false,
  "_integrity": "sha1-s/RpLxVrVCEApFfhrBzsT2znYP4=",
  "_location": "/gulp-useref",
  "_phantomChildren": {
    "clone-stats": "0.0.1",
    "core-util-is": "1.0.2",
    "duplexify": "3.7.1",
    "gaze": "0.5.2",
    "glob2base": "0.0.12",
