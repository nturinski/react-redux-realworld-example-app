    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../color-support/bin.js" "$@"
  ret=$?
else 
  node  "$basedir/../color-support/bin.js" "$@"
  ret=$?
fi
exit $ret
