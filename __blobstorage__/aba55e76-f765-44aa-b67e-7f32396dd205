	@COVERAGE=1 ISTANBUL_REPORTERS=lcovonly ./node_modules/.bin/mocha --reporter mocha-istanbul
	@cat lcov.info | ./node_modules/.bin/coveralls
	@rm -rf lib-cov lcov.info

travis: jshint test coveralls

