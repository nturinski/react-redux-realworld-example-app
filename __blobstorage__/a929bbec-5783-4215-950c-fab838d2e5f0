#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../defs/build/es5/defs" "$@"
  ret=$?
else 
  node  "$basedir/../defs/build/es5/defs" "$@"
  ret=$?
fi
exit $ret
