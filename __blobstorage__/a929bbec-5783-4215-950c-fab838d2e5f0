  "_id": "postcss@4.1.16",
  "_inBundle": false,
  "_integrity": "sha1-TESbTIr53zyvbTf44eV10DYXWNw=",
  "_location": "/postcss",
  "_phantomChildren": {
    "amdefine": "1.0.1"
  },
  "_requested": {
