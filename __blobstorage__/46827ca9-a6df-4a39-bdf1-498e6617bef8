	    var type = Object.prototype.toString.call(x).slice(8, -1).toLowerCase();
	
	    if (type === "object" && x.jquery) {
	        return "jquery";
	    }
	
	    if (type.indexOf("element") > 1) {
	        return "element";
	    }
	
	    return type;
	};
	
