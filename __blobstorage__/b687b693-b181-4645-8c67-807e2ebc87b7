    var parser = JSONStream.parse([]);
    parser.on('end', function() {
        console.log('close')
        console.error('PASSED');
        server.close();
    });
    client.pipe(parser);
    var n = 4
    client.on('data', function () {
      if(--n) return
      client.end();
    })
});
server.listen(9999);


