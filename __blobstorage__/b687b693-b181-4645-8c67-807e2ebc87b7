 * http://opensource.org/licenses/BSD-3-Clause
 */
exports.SourceMapGenerator = _dereq_(598).SourceMapGenerator;
exports.SourceMapConsumer = _dereq_(597).SourceMapConsumer;
exports.SourceNode = _dereq_(599).SourceNode;

