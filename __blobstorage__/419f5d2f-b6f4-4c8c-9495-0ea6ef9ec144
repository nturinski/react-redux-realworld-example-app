    "decodeuricomponent",
    "components",
    "decoder",
    "url"
  ],
  "license": "MIT",
  "name": "decode-uri-component",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SamVerschueren/decode-uri-component.git"
  },
  "scripts": {
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "test": "xo && nyc ava"
  },
  "version": "0.2.0"
}
'use strict';
require('./')();
{
  "name": "source-map-url",
  "version": "0.4.0",
  "author": "Simon Lydell",
  "license": "MIT",
  "description": "Tools for working with sourceMappingURL comments.",
  "keywords": [
    "source map",
    "sourceMappingURL",
    "comment",
    "annotation"
  ],
  "main": "source-map-url.js",
  "repo": "lydell/source-map-url",
  "scripts": [
    "source-map-url.js"
