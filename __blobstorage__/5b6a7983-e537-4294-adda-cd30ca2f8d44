var ReplSetManager = require('mongodb-topology-manager').ReplSet,
  f = require('util').format;

var rsOptions = {
  server: {
    keyFile: __dirname + '/test/functional/data/keyfile.txt',
    auth: null,
    replSet: 'rs'
  },
  client: {
    replSet: 'rs'
  }
