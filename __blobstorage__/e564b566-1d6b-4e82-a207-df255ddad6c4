	"$schema" : "http://json-schema.org/draft-01/hyper-schema#",
	"id" : "http://json-schema.org/draft-01/json-ref#",
	
	"items" : {"$ref" : "#"},
	"additionalProperties" : {"$ref" : "#"},
	
	"links" : [
		{
			"href" : "{$ref}",
			"rel" : "full"
		},
		
		{
			"href" : "{$schema}",
			"rel" : "describedby"
		},
		
